Algoritmo Intercambio
	Definir c Como Numerica;
	Definir n2 Como Entera;
	Definir n1 Como Entera;
	Escribir "Introduce numero 1";
	leer n1;
	Escribir "Introduce numero 2";
	leer n2;
	
	Escribir "numero 1 = ", n1;
	escribir "numero 2 = ", n2;
	
	c <- n1;
	n1 <- n2;
	n2 <- c;
	
	Escribir "numero 1 = ", n1;
	escribir "numero 2 = ", n2;
	
	
FinAlgoritmo
