Algoritmo arreglo
	
	Definir i Como Numerica;
	
	Definir primerarreglo Como Numerica;
	
	Dimension primerarreglo[5];
	
	primerarreglo[1]<- 1;
	primerarreglo[2]<- 2;
	primerarreglo[3]<- 3;
	primerarreglo[4]<- 4;
	primerarreglo[5]<- 5;
	
	Para i<-1 Hasta 5 Con Paso 1 Hacer
		Escribir "primerarreglo[",i,"] ",primerarreglo[i];
	Fin Para
	
FinAlgoritmo
